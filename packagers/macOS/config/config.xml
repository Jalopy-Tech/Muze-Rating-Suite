<?xml version="1.0" encoding="UTF-8"?>
<Installer>
	<Name>Muze Rating Suite</Name>
	<Version>1.1</Version>
	<Title>Muze Rating Suite Installer</Title>
	<Publisher>Jalopy Tech</Publisher>
	<ProductUrl>https://github.com/Jalopy-Tech/Muze-Rating-Suite/blob/main/README.md</ProductUrl>	
	<StartMenuDir>Muze Rating Suite</StartMenuDir>
	<TargetDir>@ApplicationsDir@/Muze Rating Suite</TargetDir>
	<RunProgram>@TargetDir@/Muze Rater</RunProgram>
	<RunProgramDescription>Launch Muze Rater (both Muze Rater and Muze Rating Calculator can be launched from Launcher)</RunProgramDescription>
	<MaintenanceToolName>Muze Rating Suite Maintenance Tool</MaintenanceToolName>
	<MaintenanceToolIniFile>Muze Rating Suite Maintenance Tool</MaintenanceToolIniFile>
	<InstallerApplicationIcon>muze-rating-suite</InstallerApplicationIcon>
	<WizardDefaultWidth>1000</WizardDefaultWidth>
	<WizardStyle>Modern</WizardStyle>
	<Logo>logo.png</Logo>
	<Watermark>watermark.png</Watermark>
	<ControlScript>controlscript.js</ControlScript>
	<SupportsModify>false</SupportsModify>
	<CreateLocalRepository>false</CreateLocalRepository>
</Installer>
